        self.species = species

class circle:
    pi = 3.14159
    def __init__ (self,radius=1):
        self.radius = radius
        self.area = radius**2*pi
    def setRadius(self,new_radius):
        self.radius = new_radius
        Self.area = new_radius**2*pi

c = circle

