import random
age = int(input("Enter your age here: "))
boolean = True
while boolean == True:
    if age < 0:
        print("You cannot be younger than 0")
        age = int(input("Enter your age here: "))
    else:
        print(f"You are {age} years old")
        break


Guess = int(input("Guess a number between 1 and 10: "))
number = 5
while number >= 1:
    if number == Guess:
        print("You did it!")
        break
    else:
        print("Try again!")
        Guess = int(input("Guess a number between 1 and 10: "))